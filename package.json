{
	"name": "geopulse",
	"version": "0.0.25",
	"description": "Geolocation info based on user IP",
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"server": "cd server && deno run --allow-net --allow-read --allow-write --watch main.ts",
		"process": "tsc && node dist/country.process.js"
	},
	"main": "dist/index.js",
	"keywords": [
		"geolocation",
		"ip",
		"country",
		"city",
		"currency",
		"timezone"
	],
	"homepage": "https://geopulse.pro",
	"bugs": {
		"url": "https://github.com/GeoPulseData/pulse/issues"
	},
	"author": "Ionel Lupu <ionel.cristian.lupu@gmail.com> (https://ionel-lupu.com)",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeoPulseData/pulse.git"
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"type": "module",
	"devDependencies": {
		"@types/node": "^22.7.0",
		"typescript": "^5.6.2",
		"vitest": "^2.1.1"
	},
	"dependencies": {
		"simple-xml-to-json": "^1.2.3"
	}
}
